{
  "expo": {
    "name": "Delivered",
    "slug": "Delivered",
    "description": "An app that improves the efficiency of university mailrooms",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/DeliveredIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/DeliveredSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.junie75.deliveredapp2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cb5dd61b-c887-479b-8851-4149c1d676d9"
      }
    },
    "plugins": [
      ["@morrowdigital/watermelondb-expo-plugin", { "disableJSI": true }],
      ["expo-build-properties"]
    ]
  }
}
